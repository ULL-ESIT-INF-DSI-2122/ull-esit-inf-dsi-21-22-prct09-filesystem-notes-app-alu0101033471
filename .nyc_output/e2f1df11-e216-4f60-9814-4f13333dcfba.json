    if (!fs.existsSync(`./dist/${String(argv.user)}`)){\n          fs.mkdir(path.join(__dirname, String(argv.user)), (err) => {\n            if (err) {\n              return console.error(chalk.red(err));\n            }\n          });\n        }\n        if (fs.existsSync(`./dist/${argv.user}/${argv.title}.json`)){\n          console.error(chalk.red('Note title taken!'));\n        }\n        else{\n          let info = {\n            title: String(argv.title),\n            body: String(argv.body),\n            color: argv.color\n          };\n          let data = JSON.stringify(info, null, 2);\n          setTimeout(() => {\n            fs.writeFile(`./dist/${argv.user}/${argv.title}.json`, data, (err) => {\n              if (err) {\n                return console.error(chalk.red(err));\n              }\n              else{\n                console.log(chalk.green(`New note added!`));\n              }\n            });\n          }, 1000);\n        }\n      },\n    });\n  }\n}"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"1e9acfd6c9c45363dd6d5cda12637c4240f0bea6","contentHash":"d40937989de92df1a808d32c18a9974ab9ef6a081cfe6dee9a2c2be25e943a57"},"/home/usuario/ull-esit-inf-dsi-21-22-prct09-filesystem-notes-app-alu0101033471/src/list.ts":{"path":"/home/usuario/ull-esit-inf-dsi-21-22-prct09-filesystem-notes-app-alu0101033471/src/list.ts","statementMap":{"0":{"start":{"line":2,"column":22},"end":{"line":4,"column":1}},"1":{"start":{"line":3,"column":4},"end":{"line":3,"column":62}},"2":{"start":{"line":5,"column":0},"end":{"line":5,"column":62}},"3":{"start":{"line":6,"column":0},"end":{"line":6,"column":22}},"4":{"start":{"line":7,"column":16},"end":{"line":7,"column":49}},"5":{"start":{"line":8,"column":16},"end":{"line":8,"column":49}},"6":{"start":{"line":9,"column":13},"end":{"line":9,"column":43}},"7":{"start":{"line":19,"column":8},"end":{"line":54,"column":11}},"8":{"start":{"line":37,"column":16},"end":{"line":52,"column":19}},"9":{"start