ommand: 'modify',\n      describe: 'modify a note',\n      builder: {\n        title: {\n          describe: 'Note title',\n          demandOption: true,\n          type: 'string',\n        },\n        user: {\n          describe: 'Name user',\n          demandOption: true,\n          type: 'string',\n        },\n        body: {\n          describe: 'Body of the note',\n          demandOption: true,\n          type: 'string',\n        },\n        color: {\n          describe: 'Color of the letters',\n          demandOption: true,\n          type: 'string',\n        }\n      },\n      handler(argv) {\n        if (!fs.existsSync(`./dist/${String(argv.user)}/${String(argv.title)}.json`)){\n          console.error(chalk.red(`No note found`));\n        }\n        else{\n          let info = {\n            title: String(argv.title),\n            body: String(argv.body),\n            color: argv.color\n          };\n          let data = JSON.stringify(info, null, 2);\n            fs.writeFile(`./dist/${argv.user}/${argv.title}.json`, data, (err) => {\n              if (err) {\n                return console.error(chalk.red(err));\n              }\n              else{\n                console.log(chalk.green(`Modified note!`));\n              }\n            });\n        }\n      },\n    });\n  }\n}"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"275d320eadc83ef0ba5e0427665dbe1816ecf3a3","contentHash":"dee6e6622dc7d3f81cf759a9f18346edf7c2c1026a34e390d8eaa1f8cd87677c"}}